  },

  "apiReport": {
    "enabled": true,

    "reportFileName": "strict-store.api.md"

    // "reportVariants": ["public", "beta"],

    // "reportFolder": "<projectFolder>/etc/",

    // "reportTempFolder": "<projectFolder>/temp/",

    // "includeForgottenExports": false
  },

  "docModel": {
